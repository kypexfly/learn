{
    "version": 2,
    "buildCommand": "pnpm run build",
    "builds": [
        {
            "src": "build/index.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["build/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "build/index.js"
        }
    ]
}